{
  "cli": {
    "version": ">= 3.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true,
        "cocoapods": "1.15.2"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "image": "macos-sonoma-14.5-xcode-15.4",
        "cocoapods": "1.15.2"
      }
    },
    "production": {
      "ios": {
        "image": "macos-sonoma-14.5-xcode-15.4",
        "cocoapods": "1.15.2",
        "distribution": "store",
      },
      "env": {
          "USE_FRAMEWORKS": "static",
          "CLANG_CXX_LANGUAGE_STANDARD": "gnu++20",
          "CLANG_CXX_LIBRARY": "libc++",
          "GCC_PREPROCESSOR_DEFINITIONS": "_LIBCPP_DISABLE_AVAILABILITY=1",
          "OTHER_CFLAGS": "-fno-omit-frame-pointer -fexceptions -frtti"
      }
    }
  },
  "submit": {
    "production": {}
  }
}