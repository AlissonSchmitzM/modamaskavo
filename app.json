{
  "name": "modamaskavo",
  "displayName": "Moda Maskavo",
  "expo": {
    "name": "Moda Maskavo",
    "slug": "modamaskavo",
    "version": "1.0.9",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "owner": "alissonschmitz7",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "jsEngine": "jsc",
    "ios": {
      "deploymentTarget": "17.0",
      "supportsTablet": true,
      "bundleIdentifier": "com.modamaskavo",
      "buildNumber": "1.0.9",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "Permite selecionar fotos para o aplicativo",
        "NSCameraUsageDescription": "Permite tirar fotos para o aplicativo",
        "NSMicrophoneUsageDescription": "Permite gravar áudio quando tirar vídeos",
        "UIUserInterfaceStyle": "Automatic"
      },
      "config": {
        "usesNonExemptEncryption": false,
        "googleSignIn": {
          "reservedClientId": "582893482320-35hmr955sm4fm3dudgu1dg7ouvge3d44.apps.googleusercontent.com"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.modamaskavo",
      "versionCode": 8,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "18.0",
			"buildConfiguration": {
			  "CLANG_CXX_LANGUAGE_STANDARD": "c++20",
			  "CLANG_CXX_LIBRARY": "libc++",
			  "GCC_C_LANGUAGE_STANDARD": "c11",
			  "GCC_PREPROCESSOR_DEFINITIONS": "_LIBCPP_DISABLE_AVAILABILITY=1",
			  "HEADER_SEARCH_PATHS": "\"$(PODS_ROOT)/Headers/Public/React-bridging\" \"$(PODS_ROOT)/Headers/Public/React-hermes\" \"$(PODS_ROOT)/DoubleConversion\" \"$(PODS_ROOT)/Headers/Public/DoubleConversion\""
            }
          },
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0"
          }
        }
      ],
      "@react-native-firebase/app"
    ],
    "extra": {
      "eas": {
        "projectId": "6644fae0-c4a7-4999-b252-3ba24e6c0546"
      }
    }
  }
}